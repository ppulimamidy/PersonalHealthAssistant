{
  "id": null,
  "title": "Medical Analysis Service Dashboard",
  "tags": ["medical-analysis", "ai", "healthcare"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Medical Analysis Requests",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(medical_analysis_requests_total{job=\"medical-analysis-service\"}[5m])",
          "legendFormat": "Requests/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "displayMode": "list",
            "orientation": "auto"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "AI Model Performance",
      "type": "stat",
      "targets": [
        {
          "expr": "ai_model_accuracy{job=\"medical-analysis-service\"}",
          "legendFormat": "Model Accuracy"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 80},
              {"color": "green", "value": 90}
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Analysis Types",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(medical_analysis_by_type_total{job=\"medical-analysis-service\"}[5m])",
          "legendFormat": "{{analysis_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "AI Processing Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(ai_processing_duration_seconds_bucket{job=\"medical-analysis-service\"}[5m]))",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "histogram_quantile(0.50, rate(ai_processing_duration_seconds_bucket{job=\"medical-analysis-service\"}[5m]))",
          "legendFormat": "50th percentile"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 5,
      "title": "Confidence Scores",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(ai_confidence_scores_bucket{job=\"medical-analysis-service\"}[5m]))",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "histogram_quantile(0.50, rate(ai_confidence_scores_bucket{job=\"medical-analysis-service\"}[5m]))",
          "legendFormat": "50th percentile"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "percent"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    },
    {
      "id": 6,
      "title": "Response Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"medical-analysis-service\"}[5m]))",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"medical-analysis-service\"}[5m]))",
          "legendFormat": "50th percentile"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 7,
      "title": "Error Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_requests_total{job=\"medical-analysis-service\", status=~\"4..|5..\"}[5m])",
          "legendFormat": "Error Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      }
    },
    {
      "id": 8,
      "title": "Model Predictions",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(ai_predictions_total{job=\"medical-analysis-service\"}[5m])",
          "legendFormat": "Predictions/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 32
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s"
} 